{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vuetify-swatches",
  "version": "1.0.7",
  "license": "MIT",
  "description": "Color Swatch component for Vuetify.",
  "keywords": [
    "vuejs",
    "vue",
    "vue-components",
    "color-picker",
    "color-palette",
    "swatches",
    "vue2",
    "vue-plugin",
    "vue-resource",
    "vuetify",
    "vuetify-components"
  ],
  "type": "module",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vuetify-swatches",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vuetify-swatches.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vuetify-swatches/issues"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "browser": {
    "sfc": "src/components/VSwatches.vue"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "src/components/VSwatches.vue"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.14.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "type-check": "tsc --noEmit --composite false",
    "build": "run-p type-check build-only build-declaration",
    "build:analyze": "vite build --mode=analyze",
    "build:clean": "rimraf dist docs",
    "build:docs": "vite build --mode=docs",
    "build-only": "vite build",
    "build-declaration": "tsc -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --mode=docs",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.6.14 || <3",
    "vuetify": "^2.6.14"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@codemirror/lang-vue": "^0.1.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "1.32.12",
    "stylelint": "^15.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.5.5",
    "vue": "^2.7.13",
    "vue-codemirror6": "^1.1.11",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24",
    "vuetify": "^2.6.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "http-cache-semantics": "^4.1.1"
  }
}
