{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vuetify-swatches",
  "version": "1.6.4",
  "description": "Color Swatch component for Vuetify3.",
  "keywords": [
    "vuejs",
    "vue",
    "vue-components",
    "color-picker",
    "color-palette",
    "swatches",
    "vue3",
    "vue-plugin",
    "vue-resource",
    "vuetify",
    "vuetify3",
    "vuetify-components"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vuetify-swatches",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vuetify-swatches.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vuetify-swatches/issues"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "jsdelivr": "dist/index.iife.js",
  "unpkg": "dist/index.iife.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "browser": {
    "./sfc": "./src/components/VSwatches.vue"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "jsdelivr": "./dist/index.iife.js",
      "unpkg": "./dist/index.iife.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "jsdelivr": "./dist/style.css",
      "unpkg": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "src/components/VSwatches.vue"
  ],
  "engines": {
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.2.2",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "type-check": "vue-tsc --build --force",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode=analyze",
    "build:clean": "rimraf dist docs",
    "build:docs": "vite build --mode=docs",
    "build-only": "vite build",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --mode=docs",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "peerDependencies": {
    "vuetify": "^3.5.17"
  },
  "devDependencies": {
    "@codemirror/lang-vue": "0.1.2",
    "@eslint/js": "^9.4.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vuejs-accessibility": "^2.3.0",
    "eslint-plugin-vuetify": "^2.4.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "vite": "^5.2.13",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.27",
    "vue-codemirror6": "^1.3.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.21",
    "vuetify": "^3.6.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "get-func-name": ">=2.0.2",
    "ip": ">=2.0.1",
    "json5": ">=2.2.3",
    "lodash": ">=4.17.21",
    "postcss": ">=8.4.31",
    "semver": ">=7.5.3",
    "tar": ">=6.2.1",
    "vite": ">=5.0.12",
    "yaml": ">=2.3.2"
  }
}
