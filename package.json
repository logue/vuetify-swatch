{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vuetify-swatches",
  "private": false,
  "version": "0.0.2",
  "description": "Color Swatch component for Vuetify.",
  "keywords": [
    "vuejs",
    "vue",
    "vue-components",
    "color-picker",
    "color-palette",
    "swatches",
    "vue2",
    "vue-plugin",
    "vue-resource",
    "vuetify",
    "vuetify-components"
  ],
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vuetify-swatches",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vuetify-swatches.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vuetify-swatches/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "main": "./dist/VSwatches.umd.js",
  "module": "./dist/VSwatches.es.js",
  "browser": {
    "./sfc": "./src/components/VSwatches.vue"
  },
  "exports": {
    ".": {
      "import": "./dist/VSwatches.es.js",
      "require": "./dist/VSwatches.umd.js"
    }
  },
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/src/components/VSwatches.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build & vue-tsc",
    "build:clean": "rimraf dist && rimraf types",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "lint:style": "stylelint **/*.{css,sass,scss,htm,html,vue} --fix --cache",
    "lint:markup": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-vuetify": "^0.6.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "eslint-plugin-vuetify": "^1.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.1",
    "prettier-plugin-jsdoc": "^0.3.34",
    "prettier-plugin-md-nocjsp": "^1.2.0",
    "prettier-plugin-sort-imports": "^1.6.0",
    "rimraf": "^3.0.2",
    "sass": "1.32.12",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.6.3",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-stylelint": "^2.1.0",
    "vite-plugin-vue2": "^1.9.3",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-codemirror6": "0.1.0",
    "vue-eslint-parser": "^8.3.0",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.33.9",
    "vuetify": "^2.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": "vue-tsc --noEmit",
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  }
}
