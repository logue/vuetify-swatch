{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vuetify-swatches",
  "version": "1.3.4",
  "description": "Color Swatch component for Vuetify3.",
  "keywords": [
    "vuejs",
    "vue",
    "vue-components",
    "color-picker",
    "color-palette",
    "swatches",
    "vue3",
    "vue-plugin",
    "vue-resource",
    "vuetify",
    "vuetify3",
    "vuetify-components"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vuetify-swatches",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vuetify-swatches.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vuetify-swatches/issues"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "browser": {
    "./sfc": "./src/components/VSwatches.vue"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "src/components/VSwatches.vue"
  ],
  "engines": {
    "node": ">=18.12.1",
    "yarn": ">=1.22.10"
  },
  "packageManager": "yarn@3.3.0",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite ",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "build": "run-p type-check build-only build-declaration",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build:docs": "vite build --config vite.config.docs.ts",
    "build-only": "vite build",
    "build-declaration": "vue-tsc --composite false --declaration --emitDeclarationOnly --declarationDir ./dist",
    "preview": "vite preview --config vite.config.docs.ts",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.2.45",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@codemirror/lang-html": "^6.4.0",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/tsconfig": "^0.1.3",
    "codemirror": "^6.0.1",
    "eslint": "^8.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.2",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "1.56.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-banner": "^0.6.1",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-vuetify": "^1.0.0",
    "vue": "^3.2.45",
    "vue-codemirror6": "1.1.1",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.11",
    "vuetify": "^3.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": "vue-tsc --noEmit",
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.8.1"
  }
}
